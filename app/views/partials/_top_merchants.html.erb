<h5 style="text-align: center;">Top 5 Merchants</h5><br>
<table style='width: 100%; border-color: white;'>
  <tr>
    <th>Merchant</th>
    <th>Revenue</th>
  </tr>
    <% @top_five_merchants.each do |merchant| %>
      <tr>
        <td><%= "#{merchant.name}" %></td>
        <td><%= "#{merchant.revenue}" %></td>
      </tr>
    <% end %>
</table><br>