<%= form_with url: path, method: method, local: true do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %><br/><br/>
  <%= f.label :description %>
  <%= f.text_field :description %><br/><br/>
  <%= f.label :unit_price %>
  <%= f.text_field :unit_price %><br/><br/>
  <%= f.submit 'Update'%>
<% end %>
